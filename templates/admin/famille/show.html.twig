<!DOCTYPE html>

<title>Famille</title>

{% block body %}
    <h1>Famille</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>CodeFamille</th>
                <td>{{ famille.codeFamille }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ famille.libelle }}</td>
            </tr>
            <tr>
                <th>IdSociete</th>
                <td>{{ famille.idSociete }}</td>
            </tr>
            <tr>
                <th>IdAnnee</th>
                <td>{{ famille.idAnnee }}</td>
            </tr>
            <tr>
                <th>Supprimer</th>
                <td>{{ famille.supprimer ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ famille.description }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ famille.numero }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_famille_index') }}">back to list</a>

    <a href="{{ path('app_famille_edit', {'codeFamille': famille.codeFamille}) }}">edit</a>

    {{ include('famille/_delete_form.html.twig') }}
{% endblock %}
